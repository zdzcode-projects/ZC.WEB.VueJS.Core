{
  "name": "@zdzcode/zc-web-vuejs-core",
  "version": "0.5.2",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve ./dev/main.js",
    "build:demo": "vue-cli-service build --dest demo ./dev/main.js",
    "build": "vue-cli-service build --target lib --name zdzcode src/plugin.js",
    "build:local": "vue-cli-service build --target lib --name zdzcode src/plugin.js && npm pack",
    "lint": "vue-cli-service lint --fix && vue-cli-service lint dev --fix",
    "deploy:demo": "vue-cli-service build --dest demo ./dev/main.js && ./deploy.demo.sh"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/zdzcode.common.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zdzcode-projects/ZC.WEB.VueJS.Core"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-service": "^4.5.13",
    "core-js": "3.16.3",
    "sass": "^1.38.1",
    "sass-loader": "^12.1.0",
    "vue": "2.6.14",
    "vue-cli-plugin-vuetify": "^2.4.2",
    "vue-template-compiler": "2.6.14",
    "vuetify": "2.5.8",
    "vuetify-loader": "^1.7.2"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    }
  }
